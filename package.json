{
  "name": "frasco",
  "version": "0.8.17",
  "description": "Jekyll starter project including full setup for gulp, Sass, Autoprefixer, PostCSS, webpack, imagemin, Browsersync etc.",
  "main": "gulpfile.js",
  "scripts": {
    "preinstall": "bundle install --path vendor/bundle",
    "start": "bundle exec gulp",
    "build": "bundle exec gulp build --jekyllEnv='production' --mode='production'",
    "deploy": "git checkout prod && git merge master",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ixkaito/frasco.git"
  },
  "keywords": [
    "Autoprefixer",
    "Browsersync",
    "gulp",
    "imagemin",
    "Jekyll",
    "PostCSS",
    "Sass",
    "Webpack"
  ],
  "author": "Kite",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ixkaito/frasco/issues"
  },
  "homepage": "https://ixkaito.github.io/frasco",
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "autoprefixer": "^9.0.1",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.6",
    "babel-preset-env": "^1.7.0",
    "bootstrap": "^4.3.1",
    "browser-sync": "^2.26.7",
    "cryptiles": ">=4.1.2",
    "css-loader": "^3.2.0",
    "del": "^5.0.0",
    "eslint-config-google": "^0.9.1",
    "eslint-loader": "^2.1.0",
    "file-loader": "^4.2.0",
    "gulp": "^4.0.2",
    "gulp-babel": "^7.0.1",
    "gulp-concat-css": "^3.1.0",
    "gulp-eslint": "^5.0.0",
    "gulp-imagemin": "^4.1.0",
    "gulp-newer": "^1.4.0",
    "gulp-plumber": "^1.2.0",
    "gulp-postcss": "^7.0.1",
    "gulp-sass": "^4.0.2",
    "gulp-watch": "^5.0.0",
    "image-webpack-loader": "^5.0.0",
    "imagemin-pngquant": "^6.0.0",
    "jquery": "^3.4.0",
    "natives": "^1.1.6",
    "popper.js": "^1.15.0",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^7.2.0",
    "style-loader": "^1.0.0",
    "ts-loader": "^6.0.4",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "vinyl-named": "^1.1.0",
    "webpack": "^4.16.1",
    "webpack-stream": "^5.0.0",
    "yargs": "^12.0.1"
  }
}
